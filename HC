AutoDropTS.Name = "AutoDropTS"
AutoDropTS.Parent = TextLabel_4
AutoDropTS.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
AutoDropTS.BorderColor3 = Color3.fromRGB(139, 139, 139)
AutoDropTS.Position = UDim2.new(1, 0, 0, 0)
AutoDropTS.Size = UDim2.new(0.315315336, 0, 1, 0)
AutoDropTS.Font = Enum.Font.Merriweather
AutoDropTS.Text = "Exct"
AutoDropTS.TextColor3 = Color3.fromRGB(255, 0, 0)
AutoDropTS.TextScaled = true
AutoDropTS.TextSize = 14.000
AutoDropTS.TextWrapped = true
AutoDropTS.MouseButton1Click:Connect(function()
	local Players = game:GetService("Players")

	local function dropTool(tool)
		if tool and tool:IsA("Tool") then
			tool.Parent = workspace
			tool.Handle.CFrame = game.Players.LocalPlayer.Character.PrimaryPart.CFrame * CFrame.new(0, -5, 0)
		end
	end

	local function onPlayerRespawn()
		wait(1)

		local player = game.Players.LocalPlayer
		local backpack = player:FindFirstChild("Backpack")
		local trainTool = backpack and backpack:FindFirstChild("Train")

		if trainTool then
			trainTool.Parent = player.Character

			wait(0.2)

			dropTool(trainTool)
		end
	end

	local function setupPlayerRespawn()
		local player = Players.LocalPlayer

		local function setupAndRespawn()
			onPlayerRespawn()

			player.CharacterAdded:Connect(function(character)
				onPlayerRespawn()
			end)

			player.ChildAdded:Connect(function(child)
				if child:IsA("Model") and child.Name == "Player" then
					onPlayerRespawn()
				end
			end)
		end

		if player.Character then
			setupAndRespawn()
		else
			player.CharacterAdded:Connect(setupAndRespawn)
		end
	end

	setupPlayerRespawn()
end)

